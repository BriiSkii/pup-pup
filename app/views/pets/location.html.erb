<div class="row">
  <div class="col-md-3">
    <%= render 'pet_menu' %>
  </div>
  <div class="col-md-9">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3>Zip Code</h3>
      </div>

      <div class="panel-body">
        <div class="container">
          <%= form_for @pet do |f| %>
            <div class="row">
              <div class="form-group">
                <div class="form-group">
                  <!-- <%= f.label :address %><br /> -->
                  <%= f.text_field :address, autofocus: true, class: "form-control", placeholder: "Zip Code" %>
                </div>

                <div class="text-center">
                  <%= f.submit "Save", class: "btn btn-primary" %>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
